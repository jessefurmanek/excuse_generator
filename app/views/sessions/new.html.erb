<h1>Login</h1>

 <div class="container">

 	<%= bootstrap_form_tag url: login_path, method: "post" do |f| %>
        <%= f.email_field :email, class: 'form_control' %>
        <%= f.password_field :password, class: 'form_control'%>
        <%= f.submit "Log in", class: "btn" %>
      <% end %>
    
 </div>


    
    